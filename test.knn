assign int i = 0;
while (true)
{
	i = i + 1;
	if (i == 5)
	{
		continue;
	} endif;
	if (i == 10)
	{
		break;
	} endif;
	writeln(i);
} endwhile;

writeln("while test end");

for (assign int k = 0; k < 10; k = k + 1)
{
	if (k == 5)
	{
		continue;
	} endif;
	if (k == 10)
	{
		break;
	} endif;
	writeln(k);
} endfor;

writeln("for test end");